    "@fastify/multipart": "^7.7.3",
    "@fastify/rate-limit": "^10.1.0",
    "@prisma/client": "^5.0.0",
    "@the-scientist/schemas": "workspace:*",
    "@types/bcryptjs": "^3.0.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
