    "ioredis": "^5.3.0",
    "minio": "^7.1.0",
    "openai": "^4.20.0",
    "argon2": "^0.31.0",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/jsonwebtoken": "^9.0.0",
